    <packageId>musicexpanded.framework</packageId>
    <supportedVersions>
        <li>1.3</li>
        <li>1.4</li>
    </supportedVersions>
    <loadAfter>
        <li>brrainz.harmony</li>
    </loadAfter>
    <modDependencies>        
        <li>
            <packageId>brrainz.harmony</packageId>
